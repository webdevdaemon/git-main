{
	"ecmaVersion": "7",
	"libs": [
		"browser"
	],
	"loadEagerly": ["src/**"],
	"dontLoad": [
		"node_module/**"
	],
	"plugins": {
		"node": {
			"dontLoad": "",
			"load": "",
			"modules": ""
		},
		"node_resolve": {},
		"modules": {
			"dontLoad": "",
			"load": "",
			"modules": ""
		},
		"es_modules": {},
		"complete_strings": {
			"maxLength": 15
		}
	}
}
